{
  "__inputs": [],
  "__requires": [
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": "9.0.0"
    }
  ],
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "iteration": 1,
  "links": [],
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum by (stage) (increase(rca_pipeline_stage_total{tenant=~\"${tenant}\"}[$__interval]))",
          "legendFormat": "{{stage}}",
          "refId": "A"
        }
      ],
      "title": "Stage Throughput",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le, stage) (rate(rca_pipeline_stage_duration_seconds_bucket{tenant=~\"${tenant}\"}[5m])))",
          "legendFormat": "P50 {{stage}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le, stage) (rate(rca_pipeline_stage_duration_seconds_bucket{tenant=~\"${tenant}\"}[5m])))",
          "legendFormat": "P95 {{stage}}",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le, stage) (rate(rca_pipeline_stage_duration_seconds_bucket{tenant=~\"${tenant}\"}[5m])))",
          "legendFormat": "P99 {{stage}}",
          "refId": "C"
        }
      ],
      "title": "Stage Latency (P50/P95/P99)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "sum by (stage) (increase(rca_pipeline_stage_total{tenant=~\"${tenant}\",status=\"failure\"}[5m])) / sum by (stage) (increase(rca_pipeline_stage_total{tenant=~\"${tenant}\"}[5m]))",
          "legendFormat": "{{stage}}",
          "refId": "A"
        }
      ],
      "title": "Stage Error Rate",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "sum by (file_type) (increase(rca_pipeline_stage_total{tenant=~\"${tenant}\",stage=\"ingest\"}[$__interval]))",
          "legendFormat": "{{file_type}}",
          "refId": "A"
        }
      ],
      "title": "Ingest Volume by File Type",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "sum by (stage, status) (increase(rca_pipeline_stage_total{tenant=~\"${tenant}\"}[1h]))",
          "legendFormat": "{{stage}} {{status}}",
          "refId": "A"
        }
      ],
      "title": "Stage Outcomes (Hourly)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 22 },
      "targets": [
        {
          "expr": "sum by (platform) (rate(rca_platform_detection_total{tenant=~\"${tenant}\"}[5m]))",
          "legendFormat": "{{platform}}",
          "refId": "A"
        }
      ],
      "title": "Platform Detection Rate",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 22 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le, platform) (rate(rca_platform_detection_confidence_bucket{tenant=~\"${tenant}\"}[5m])))",
          "legendFormat": "P50 {{platform}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le, platform) (rate(rca_platform_detection_confidence_bucket{tenant=~\"${tenant}\"}[5m])))",
          "legendFormat": "P95 {{platform}}",
          "refId": "B"
        }
      ],
      "title": "Platform Detection Confidence (P50/P95)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 30 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, platform) (rate(rca_platform_detection_duration_seconds_bucket{tenant=~\"${tenant}\"}[5m])))",
          "legendFormat": "P95 {{platform}}",
          "refId": "A"
        }
      ],
      "title": "Platform Detection Duration (P95)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "unit": "reqps"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": "/blocked.*/"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "/passed.*/"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 30 },
      "targets": [
        {
          "expr": "sum by (status, archive_type) (rate(rca_archive_extraction_guardrails{tenant=~\"${tenant}\"}[5m]))",
          "legendFormat": "{{status}} {{archive_type}}",
          "refId": "A"
        }
      ],
      "title": "Archive Extraction Guardrail Status",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 50,
                "color": "yellow"
              },
              {
                "value": 80,
                "color": "red"
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 38 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, archive_type) (rate(rca_archive_decompression_ratio_bucket{tenant=~\"${tenant}\"}[5m])))",
          "legendFormat": "P95 {{archive_type}}",
          "refId": "A"
        }
      ],
      "title": "Archive Decompression Ratio (P95)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 38 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, archive_type) (rate(rca_archive_member_count_bucket{tenant=~\"${tenant}\"}[5m])))",
          "legendFormat": "P95 {{archive_type}}",
          "refId": "A"
        }
      ],
      "title": "Archive Member Count (P95)",
      "type": "timeseries"
    }
  ],
  "refresh": "5s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["telemetry", "pipeline"],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROMETHEUS",
        "query": "prometheus",
        "current": {
          "text": "Prometheus",
          "value": "Prometheus"
        },
        "hide": 0
      },
      {
        "type": "query",
        "name": "tenant",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "definition": "label_values(rca_pipeline_stage_total, tenant)",
        "includeAll": true,
        "multi": true,
        "refresh": 1,
        "sort": 1,
        "current": {
          "text": "All",
          "value": [".*"]
        }
      }
    ]
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m"
    ]
  },
  "timezone": "",
  "title": "Telemetry / Pipeline Overview",
  "uid": "telemetry-pipeline-overview",
  "version": 1
}
