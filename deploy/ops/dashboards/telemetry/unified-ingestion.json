{
  "dashboard": {
    "title": "RCA Unified Ingestion Enhancements",
    "tags": ["rca", "ingestion", "platform-detection", "archives"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Platform Detection Overview",
        "type": "row",
        "collapsed": false,
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Platform Detection Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(rca_platform_detection_total[5m])",
            "legendFormat": "{{platform}} - {{method}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 1
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "calcs": ["mean", "lastNotNull"],
            "displayMode": "table",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 3,
        "title": "Platform Detection by Platform",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (platform) (rca_platform_detection_total)",
            "legendFormat": "{{platform}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 1
        },
        "options": {
          "legend": {
            "display": true,
            "placement": "right",
            "values": ["value", "percent"]
          }
        }
      },
      {
        "id": 4,
        "title": "Platform Detection Confidence Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum(rate(rca_platform_detection_confidence_bucket[5m])) by (le, platform)",
            "format": "heatmap",
            "legendFormat": "{{platform}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 9
        },
        "options": {
          "calculate": true,
          "calculation": {
            "xBuckets": {
              "mode": "size",
              "value": "0.1"
            }
          },
          "color": {
            "mode": "scheme",
            "scheme": "Spectral"
          }
        }
      },
      {
        "id": 5,
        "title": "Platform Detection Duration (p50, p95, p99)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(rca_platform_detection_duration_seconds_bucket[5m])) by (le, platform))",
            "legendFormat": "{{platform}} p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(rca_platform_detection_duration_seconds_bucket[5m])) by (le, platform))",
            "legendFormat": "{{platform}} p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(rca_platform_detection_duration_seconds_bucket[5m])) by (le, platform))",
            "legendFormat": "{{platform}} p99",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 9
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Archive Extraction Overview",
        "type": "row",
        "collapsed": false,
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 17
        }
      },
      {
        "id": 7,
        "title": "Archive Extraction Guardrail Status",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(rca_archive_extraction_outcomes_total[5m])",
            "legendFormat": "{{status}} - {{archive_type}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 18
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": "/blocked.*/"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "red"
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byRegexp",
                "options": "/passed.*/"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "green"
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "id": 8,
        "title": "Guardrail Block Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(rca_archive_extraction_outcomes_total{status=~\"blocked.*\"}[5m])) / sum(rate(rca_archive_extraction_outcomes_total[5m]))",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 18
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 0.05,
                  "color": "yellow"
                },
                {
                  "value": 0.1,
                  "color": "red"
                }
              ]
            }
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "background"
        }
      },
      {
        "id": 9,
        "title": "Total Extractions (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(rca_archive_extraction_outcomes_total[24h]))",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 18
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            }
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "id": 10,
        "title": "Decompression Ratio Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum(rate(rca_archive_decompression_ratio_bucket[5m])) by (le, archive_type)",
            "format": "heatmap",
            "legendFormat": "{{archive_type}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 22
        },
        "options": {
          "calculate": true,
          "calculation": {
            "xBuckets": {
              "mode": "size",
              "value": "10"
            }
          },
          "color": {
            "mode": "scheme",
            "scheme": "RdYlGn",
            "reverse": true
          },
          "yAxis": {
            "decimals": 0
          }
        }
      },
      {
        "id": 11,
        "title": "Archive Member Count Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(rca_archive_member_count_bucket[5m])) by (le, archive_type))",
            "legendFormat": "{{archive_type}} p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(rca_archive_member_count_bucket[5m])) by (le, archive_type))",
            "legendFormat": "{{archive_type}} p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(rca_archive_member_count_bucket[5m])) by (le, archive_type))",
            "legendFormat": "{{archive_type}} p99",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 26
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Average Decompression Ratio by Archive Type",
        "type": "bargauge",
        "targets": [
          {
            "expr": "avg(rate(rca_archive_decompression_ratio_sum[5m])) by (archive_type) / avg(rate(rca_archive_decompression_ratio_count[5m])) by (archive_type)",
            "legendFormat": "{{archive_type}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 30
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 50,
                  "color": "yellow"
                },
                {
                  "value": 80,
                  "color": "red"
                }
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 13,
        "title": "Alerts & Thresholds",
        "type": "row",
        "collapsed": false,
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 38
        }
      },
      {
        "id": 14,
        "title": "High Decompression Ratio Alerts",
        "type": "table",
        "targets": [
          {
            "expr": "rca_archive_decompression_ratio > 80",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 39
        },
        "options": {
          "showHeader": true
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "indexByName": {
                "archive_type": 0,
                "job_id": 1,
                "Value": 2
              },
              "renameByName": {
                "archive_type": "Archive Type",
                "job_id": "Job ID",
                "Value": "Ratio"
              }
            }
          }
        ]
      },
      {
        "id": 15,
        "title": "Platform Detection Failures",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (platform, error_type) (rca_platform_detection_total{platform=\"unknown\"})",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 39
        },
        "options": {
          "showHeader": true
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "platform",
          "label": "Platform",
          "type": "query",
          "query": "label_values(rca_platform_detection_total, platform)",
          "multi": true,
          "includeAll": true
        },
        {
          "name": "archive_type",
          "label": "Archive Type",
          "type": "query",
          "query": "label_values(rca_archive_extraction_outcomes_total, archive_type)",
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "changes(rca_build_info[5m]) > 0",
          "iconColor": "blue"
        }
      ]
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m"]
    }
  }
}
